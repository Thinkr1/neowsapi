{
    "plugins": [
      "react"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "sourceType": "module",
      "requireConfigFile": false,
      "ecmaFeatures": {
        "jsx": true,
        "modules": true
      }
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "rules": {
      "react/prop-types": "off",
      "react/react-in-jsx-scope": "off",
      "no-unused-vars": "off"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }